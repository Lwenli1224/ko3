#!/usr/bin/env node

require('commander')
  .version(require('../package').version)
  .usage('<command> [options]')
  .command('build', 'Build the project')
  .command('serve', 'Serve the project')
  .command('new', 'Create a new project or file')
  .command('init', 'Init project')
  .parse(process.argv)
